<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
	<appSettings>
		<!-- big / medium / small -->
		<add key="size" value="big"/>
		<add key="filter-common" value="@SansMouse Foursquare ipo48 Foursquare" />
		<add key="filter-nort" value="allRT" />
		<add key="filter-notimeline" value="allTimeline" />
		<add key="filter-morning" value="filter-common twidaq @ch9 @podnikatel Fera/Looney @smartmania @lidovenoviny @TNEdge" />
	</appSettings>
	<log4net>
		<appender name="udpAppender" type="log4net.Appender.UdpAppender">
			<param name="RemoteAddress" value="127.0.0.1" />
			<param name="RemotePort" value="8080" />
			<layout type="log4net.Layout.XmlLayout">
				<param name="Prefix" value="" />
			</layout>
		</appender>
		<root>
			<appender-ref ref="udpAppender" />
			<level value="OFF" />
		</root>
	</log4net>
</configuration>